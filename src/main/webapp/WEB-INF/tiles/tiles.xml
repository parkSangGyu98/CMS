<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN" "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    
	<definition name="doubleS" template="/WEB-INF/tiles/layout/layout.jsp">
		<put-attribute name="header"  value="/WEB-INF/tiles/layout/header.jsp"/>
		<put-attribute name="content" value="" />
		<put-attribute name="leftBar"  value="/WEB-INF/tiles/layout/leftBar.jsp"/>
		<put-attribute name="footer"  value="/WEB-INF/tiles/layout/footer.jsp"/>
	</definition>
	
	
	<definition name="project/*" extends="doubleS">
		<put-attribute name="content" value="/WEB-INF/views/project/{1}.jsp"/>
	</definition>
	<definition name="project/*/*" extends="doubleS">
		<put-attribute name="content" value="/WEB-INF/views/project/{1}/{2}.jsp"/>
	</definition>
	<definition name="project/*/*/*" extends="doubleS">
		<put-attribute name="content" value="/WEB-INF/views/project/{1}/{2}/{3}.jsp"/>
	</definition>
	<definition name="project/*/*/*/*" extends="doubleS">
		<put-attribute name="content" value="/WEB-INF/views/project/{1}/{2}/{3}/{4}.jsp"/>
	</definition>
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                         
</tiles-definitions>